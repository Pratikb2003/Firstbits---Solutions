#include <stdio.h>

void main() {
    int hour;
    printf("Enter hour (0â€“23): ");
    scanf("%d", &hour);

    if(hour >= 5 && hour < 12)
        printf("Morning");
    else if(hour >= 12 && hour < 17)
        printf("Afternoon");
    else if(hour >= 17 && hour < 21)
        printf("Evening");
    else
        printf("Night");
}