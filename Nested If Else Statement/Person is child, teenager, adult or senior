#include<stdio.h>
int main()
{
	int age;
	
	printf("Enter the age: ");
	scanf("%d" , &age);
	
	if(age < 12)
	{
		printf("%d Child\n" , age);
	}
	else if(age >= 12 && age <= 19)
	{
		printf("%d Teenager\n" , age);
	}
	else if(age >= 20 && age <= 59)
	{
		printf("%d Adult\n" , age);
	}
	else
	{
		printf("%d Senior\n" , age);
	}
}