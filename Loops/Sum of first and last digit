#include<stdio.h>
int main()
{
	int num = 12345;
	int first_digit, last_digit, sum;
	
	last_digit = num % 10;
	
	first_digit = num;
	while(first_digit >= 10) {
		first_digit = first_digit / 10;
	}
	
	sum = first_digit + last_digit;
	
	printf("The sum of %d + %d is = %d\n" , first_digit, last_digit, sum);
}