class AutomorphicCubeRoot {

    static boolean isAutoCube(int num) {
        int cubeRoot = (int) Math.round(Math.cbrt(num));
        int temp = cubeRoot;

        while (temp != 0) {
            if (temp % 10 != num % 10)
                return false;
            temp /= 10;
            num /= 10;
        }
        return true;
    }

    public static void main(String[] args) {
        int num = 15625;

        if (isAutoCube(num))
            System.out.println(num + " is Automorphic Cube Root Number");
        else
            System.out.println(num + " is Not Automorphic Cube Root Number");
    }
}