class BuzzNumber {

    static boolean isBuzz(int num) {
        return (num % 7 == 0 || num % 10 == 7);
    }

    public static void main(String[] args) {
        int num = 27;

        if (isBuzz(num))
            System.out.println(num + " is a Buzz Number");
        else
            System.out.println(num + " is Not a Buzz Number");
    }
}