class PowerOfTwo {

    static boolean isPowerOfTwo(int num) {

        if (num <= 0)
            return false;

        return (num & (num - 1)) == 0;
    }

    public static void main(String[] args) {
        int num = 16;

        if (isPowerOfTwo(num))
            System.out.println(num + " is Power of Two");
        else
            System.out.println(num + " is Not Power of Two");
    }
}