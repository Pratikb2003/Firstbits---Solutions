class SumOfArray {

    static int sumArray(int[] arr) {
        int sum = 0;

        for (int i = 0; i < arr.length; i++) {
            sum = sum + arr[i];
        }
        return sum;
    }

    public static void main(String[] args) {
        int[] a = {5, 10, 15, 20};
        int result = sumArray(a);
        System.out.println("Sum of array = " + result);
    }
}