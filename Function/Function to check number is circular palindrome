class CircularPalindrome {

    static boolean isPalindrome(int num) {
        int rev = 0, temp = num;

        while (temp != 0) {
            rev = rev * 10 + (temp % 10);
            temp /= 10;
        }
        return rev == num;
    }

    static boolean isCircularPalindrome(int num) {
        int temp = num;
        int digits = String.valueOf(num).length();

        for (int i = 0; i < digits; i++) {

            if (!isPalindrome(temp))
                return false;

            int last = temp % 10;
            temp = (last * (int)Math.pow(10, digits - 1)) + (temp / 10);
        }
        return true;
    }

    public static void main(String[] args) {
        int num = 121;

        if (isCircularPalindrome(num))
            System.out.println(num + " is Circular Palindrome");
        else
            System.out.println(num + " is Not Circular Palindrome");
    }
}