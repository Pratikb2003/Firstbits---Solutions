class BinaryPalindrome {

    static boolean isBinaryPalindrome(int num) {
        String bin = Integer.toBinaryString(num);
        String rev = "";

        for (int i = bin.length() - 1; i >= 0; i--) {
            rev = rev + bin.charAt(i);
        }
        return bin.equals(rev);
    }

    public static void main(String[] args) {
        int num = 9;

        if (isBinaryPalindrome(num))
            System.out.println(num + " is Binary Palindrome");
        else
            System.out.println(num + " is Not Binary Palindrome");
    }
}