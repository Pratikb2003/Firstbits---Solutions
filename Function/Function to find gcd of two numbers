class GCDExample {

    static int gcd(int a, int b) {
        while (a != b) {
            if (a > b)
                a = a - b;
            else
                b = b - a;
        }
        return a;
    }

    public static void main(String[] args) {
        int result = gcd(12, 18);
        System.out.println("GCD = " + result);
    }
}