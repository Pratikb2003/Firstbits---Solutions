class CountEvenArray {

    static int countEven(int[] arr) {
        int count = 0;

        for (int i = 0; i < arr.length; i++) {
            if (arr[i] % 2 == 0)
                count++;
        }
        return count;
    }

    public static void main(String[] args) {
        int[] a = {10, 21, 34, 45, 60};
        int result = countEven(a);
        System.out.println("Even numbers count = " + result);
    }
}