class TwinPrime {

    static boolean isPrime(int num) {
        if (num < 2) return false;

        for (int i = 2; i <= Math.sqrt(num); i++) {
            if (num % i == 0)
                return false;
        }
        return true;
    }

    static boolean isTwinPrime(int num) {
        return isPrime(num) && isPrime(num + 2);
    }

    public static void main(String[] args) {
        int num = 11;

        if (isTwinPrime(num))
            System.out.println(num + " is Twin Prime");
        else
            System.out.println(num + " is Not Twin Prime");
    }
}