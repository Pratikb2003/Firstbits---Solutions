class EvilPrime {

    static boolean isPrime(int num) {
        if (num < 2) return false;

        for (int i = 2; i <= Math.sqrt(num); i++) {
            if (num % i == 0)
                return false;
        }
        return true;
    }

    static boolean isEvil(int num) {
        int count = 0;

        while (num != 0) {
            if (num % 2 == 1)
                count++;
            num /= 2;
        }
        return count % 2 == 0;
    }

    static boolean isEvilPrime(int num) {
        return isPrime(num) && isEvil(num);
    }

    public static void main(String[] args) {
        int num = 10;

        if (isEvilPrime(num))
            System.out.println(num + " is Evil Prime");
        else
            System.out.println(num + " is Not Evil Prime");
    }
}