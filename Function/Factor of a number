class Factors {

    static void printFactors(int num) {
        System.out.print("Factors: ");
        for (int i = 1; i <= num; i++) {
            if (num % i == 0)
                System.out.print(i + " ");
        }
    }

    public static void main(String[] args) {
        printFactors(12);
    }
}
