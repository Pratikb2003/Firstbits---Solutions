class AmicablePair {

    static int sumProperDivisors(int num) {
        int sum = 0;

        for (int i = 1; i <= num / 2; i++) {
            if (num % i == 0)
                sum += i;
        }
        return sum;
    }

    static boolean isAmicable(int a, int b) {
        return sumProperDivisors(a) == b && sumProperDivisors(b) == a;
    }

    public static void main(String[] args) {
        int num1 = 220;
        int num2 = 284;

        if (isAmicable(num1, num2))
            System.out.println(num1 + " and " + num2 + " are Amicable Numbers");
        else
            System.out.println(num1 + " and " + num2 + " are Not Amicable Numbers");
    }
}