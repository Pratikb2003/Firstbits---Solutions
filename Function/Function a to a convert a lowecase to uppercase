class LowerToUpper {

    static char toUpper(char ch) {
        if (ch >= 'a' && ch <= 'z')
            return (char)(ch - 32);
        else
            return ch;
    }

    public static void main(String[] args) {
        char result = toUpper('g');
        System.out.println("Uppercase = " + result);
    }
}