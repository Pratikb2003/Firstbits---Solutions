class DuckPrime {

    static boolean isPrime(int num) {
        if (num < 2) return false;

        for (int i = 2; i <= Math.sqrt(num); i++) {
            if (num % i == 0)
                return false;
        }
        return true;
    }

    static boolean isDuck(int num) {
        while (num != 0) {
            if (num % 10 == 0)
                return true;
            num /= 10;
        }
        return false;
    }

    static boolean isDuckPrime(int num) {
        return isPrime(num) && isDuck(num);
    }

    public static void main(String[] args) {
        int num = 1013;

        if (isDuckPrime(num))
            System.out.println(num + " is Duck Prime");
        else
            System.out.println(num + " is Not Duck Prime");
    }
}