class EvilNumber {

    static boolean isEvil(int num) {
        int count = 0;

        while (num != 0) {
            if (num % 2 == 1)
                count++;
            num = num / 2;
        }
        return count % 2 == 0;
    }

    public static void main(String[] args) {
        int num = 10;

        if (isEvil(num))
            System.out.println(num + " is an Evil Number");
        else
            System.out.println(num + " is Not an Evil Number");
    }
}