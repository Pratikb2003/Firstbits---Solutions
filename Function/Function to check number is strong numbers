class StrongPrime {

    static boolean isPrime(int num) {
        if (num < 2) return false;

        for (int i = 2; i <= Math.sqrt(num); i++) {
            if (num % i == 0)
                return false;
        }
        return true;
    }

    static int digitSum(int num) {
        int sum = 0;

        while (num != 0) {
            sum += num % 10;
            num /= 10;
        }
        return sum;
    }

    static boolean isStrongPrime(int num) {
        return isPrime(num) && isPrime(digitSum(num));
    }

    public static void main(String[] args) {
        int num = 23;

        if (isStrongPrime(num))
            System.out.println(num + " is Strong Prime");
        else
            System.out.println(num + " is Not Strong Prime");
    }
}