class DuckPalindrome {

    static boolean hasZero(int num) {
        while (num != 0) {
            if (num % 10 == 0)
                return true;
            num /= 10;
        }
        return false;
    }

    static boolean isPalindrome(int num) {
        int rev = 0, temp = num;

        while (temp != 0) {
            rev = rev * 10 + temp % 10;
            temp /= 10;
        }
        return rev == num;
    }

    static boolean isDuckPalindrome(int num) {
        return hasZero(num) && isPalindrome(num);
    }

    public static void main(String[] args) {
        int num = 101;

        if (isDuckPalindrome(num))
            System.out.println(num + " is Duck Palindrome");
        else
            System.out.println(num + " is Not Duck Palindrome");
    }
}