class MagicNumber {

    static int digitSum(int num) {
        int sum = 0;
        while (num != 0) {
            sum += num % 10;
            num /= 10;
        }
        return sum;
    }

    static boolean isMagic(int num) {
        while (num > 9) {
            num = digitSum(num);
        }
        return num == 1;
    }

    public static void main(String[] args) {
        int num = 19;

        if (isMagic(num))
            System.out.println(num + " is a Magic Number");
        else
            System.out.println(num + " is Not a Magic Number");
    }
}