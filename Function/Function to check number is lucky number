class LuckyNumber {

    static boolean isLucky(int num) {
        int sum = 0;

        while (num != 0) {
            sum += num % 10;
            num /= 10;
        }
        return sum == 7;
    }

    public static void main(String[] args) {
        int num = 34;

        if (isLucky(num))
            System.out.println(num + " is Lucky Number");
        else
            System.out.println(num + " is Not Lucky Number");
    }
}