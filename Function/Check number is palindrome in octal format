class OctalPalindrome {

    static boolean isOctalPalindrome(int num) {
        String oct = Integer.toOctalString(num);
        String rev = "";

        for (int i = oct.length() - 1; i >= 0; i--) {
            rev = rev + oct.charAt(i);
        }
        return oct.equals(rev);
    }

    public static void main(String[] args) {
        int num = 65;

        if (isOctalPalindrome(num))
            System.out.println(num + " is Octal Palindrome");
        else
            System.out.println(num + " is Not Octal Palindrome");
    }
}