#include <stdio.h>

int secondLargest(int arr[], int n) {
    int max1 = -99999, max2 = -99999;

    for(int i = 0; i < n; i++) {
        if(arr[i] > max1) {
            max2 = max1;
            max1 = arr[i];
        }
        else if(arr[i] > max2 && arr[i] != max1) {
            max2 = arr[i];
        }
    }

    return max2;
}

int main() {
    int a[6] = {12, 45, 67, 23, 89, 33};

    int result = secondLargest(a, 6);

    printf("Second Largest = %d", result);

    return 0;
}