#include <stdio.h>

void reverse(int arr[], int n) {
    int start = 0;
    int end = n - 1;
    int temp;

    while(start < end) {
        temp = arr[start];
        arr[start] = arr[end];
        arr[end] = temp;

        start++;
        end--;
    }
}

int main() {
    int a[5] = {1, 2, 3, 4, 5};

    reverse(a, 5);

    printf("Reversed Array: ");
    for(int i = 0; i < 5; i++) {
        printf("%d ", a[i]);
    }

    return 0;
}