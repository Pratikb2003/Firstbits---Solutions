#include<stdio.h>
#include<string.h>
int main()
{
	char str[100];
	char newStr[100];
	int i;
	int j = 0;
	printf("Enter a string: ");
	gets(str);
	for(int i = 0; i < strlen(str); i++) {
		if( i % 2 == 0) {
			newStr[j++] = str[i];
		}
	}
	newStr[j] = '\0';
	printf("String after removing odd index characters: %s\n" , newStr);
	return 0;
}