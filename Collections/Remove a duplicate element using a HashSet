import java.util.*;

public class RemoveDuplicateExample {
    public static void main(String[] args) {

        ArrayList<Integer> list = new ArrayList<>();

        list.add(10);
        list.add(20);
        list.add(10);
        list.add(30);

        HashSet<Integer> set = new HashSet<>(list);

        System.out.println("After removing duplicates: " + set);
    }
}