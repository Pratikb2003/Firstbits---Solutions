#include<stdio.h>
#include<stdlib.h>
int main()
{
	int n, i, j;
	printf("Enter size: ");
	scanf("%d" , &n);
	int *arr = (int*) malloc (n * sizeof(int));
	printf("Enter elements:\n");
	for(int i = 0; i < n; i++) {
		scanf("%d" , &arr[i]);
	}
	//sorting
	for(int i = 0; i < n - 1; i++) {
		for(int j = i + 1; j < n; j++) {
			if(arr[i] > arr[j]) {
				int temp = arr[i];
				arr[i] = arr[j];
				arr[j] = temp;
			}
		}
	}
	printf("Sorted array: ");
	for(int i = 0; i < n; i++) {
		printf("%d" , arr[i]);
	}
	free(arr);
	return 0;
}