#include<stdio.h>
#include<stdlib.h>
int main()
{
	int n, i;
	printf("Enter size: ");
	scanf("%d" , &n);
	int *arr = (int*) malloc (n * sizeof(int));
	printf("Enter elements:\n");
	for(int i = 0; i < n; i++) {
		scanf("%d" , &arr[i]);
	}
	printf("Reversed array: ");
	for(int i = n - 1; i >= 0; i--) {
		scanf("%d" , arr[i]);
	}
	free(arr);
	return 0;
}