#include<stdio.h>
#include<stdlib.h>
int main()
{
	int n, i, key;
	printf("Enter size: ");
	scanf("%d" , &n);
	int *arr = (int*)malloc(n * sizeof(int));
	printf("Enter elements:\n");
	for(int i = 0; i < n; i++) {
		scanf("%d" , &arr[i]);
	}
	printf("Enter number to search: ");
	scanf("%d" , &key);
	int found = 0;
	for(int i = 0; i < n; i++) {
		if(arr[i] == key) {
			found = 1;
			break;
		}
	}
	if(found) {
		printf("Number found\n");
	} else {
		printf("Number not found\n");
	}
	free(arr);
	return 0;
}