#include<stdio.h>
#include<stdlib.h>
int main()
{
	int n, i, j;
	int flag;
	printf("Enter size: ");
	scanf("%d" , &n);
	int *arr = (int*) malloc (n * sizeof(int));
	printf("Enter elements:\n");
	for(int i = 0; i < n; i++) {
		scanf("%d" , &arr[i]);
	}
	printf("Prime numbers: ");
    for(int i = 0; i < n; i++) {
		int num = arr[i];
		int flag = 1;
		if(num <= 1)
		continue;
	}
	for(int j = 2; i <= n / 2; j++) {
		if(n % j == 0) {
			flag = 0;
			break;
		}
	}
	if(flag == 1) {
		printf("%d" , n);
	}
	free(arr);
	return 0;
}