#include <stdio.h>

int get_string_length(char *str) {
    int length = 0;
    while (str[length] != '\0') {
        length++;
    }
    return length;
}

int main() {
    char my_string[] = "Hello, World!";
    int len = get_string_length(my_string);

    printf("The string is: %s\n", my_string);
    printf("The length of the string is: %d\n", len);

    return 0;
}
