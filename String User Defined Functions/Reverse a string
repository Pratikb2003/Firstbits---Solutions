#include <stdio.h>

void stringReverse(char str[])
{
    int i = 0, j;
    char temp;

    while (str[i] != '\0')
        i++;

    j = i - 1;
    i = 0;

    while (i < j)
    {
        temp = str[i];
        str[i] = str[j];
        str[j] = temp;
        i++;
        j--;
    }
}

int main()
{
    char str[50];

    printf("Enter string: ");
    gets(str);

    stringReverse(str);

    printf("Reversed string: %s", str);

    return 0;
}