#include <stdio.h>

int onlyAlphabets(char str[])
{
    int i = 0;

    while (str[i] != '\0')
    {
        if (!((str[i] >= 'a' && str[i] <= 'z') ||
              (str[i] >= 'A' && str[i] <= 'Z')))
            return 0;
        i++;
    }
    return 1;
}

int main()
{
    char str[50];

    printf("Enter a string: ");
    gets(str);

    if (onlyAlphabets(str))
        printf("String contains only alphabets");
    else
        printf("String contains other characters");

    return 0;
}