#include <stdio.h>

int countDigits(char str[])
{
    int i = 0, count = 0;

    while (str[i] != '\0')
    {
        if (str[i] >= '0' && str[i] <= '9')
            count++;
        i++;
    }
    return count;
}

int main()
{
    char str[50];

    printf("Enter a string: ");
    gets(str);

    printf("Number of digits = %d", countDigits(str));
    return 0;
}