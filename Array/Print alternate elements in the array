#include<stdio.h>
int main()
{
	int arr[100], size, i;
	printf("Enter the size of the array: ");
	scanf("%d" , &size);
	printf("Enter the elements of the array: ");
	for(int i = 0; i < size; i++) {
		scanf("%d" , &arr[i]);
	}
	printf("Alternate elements of the array are: ");
	for(int i = 0; i < size; i = i + 2) {
		printf("%d" , arr[i]);
	} 
	printf("\n");
}