#include<stdio.h>
int main()
{
	int isPrime(int num) {
		if(num <= 1) {
			return 0;
		}
		for(int i = 2; i <= num / 2; i++) {
			if(num % i == 0) {
				return 0;
			}
		}
		return 1;
	}
	int arr[100], size, i;
	printf("Enter the size of the array: ");
	scanf("%d" , &size);
	printf("Enter the elements of the array: ");
	for(int i = 0; i < size; i++) {
		scanf("%d" , &arr[i]);
	}
	printf("Prime numbers in the array are : ");
	for(int i = 0; i < size; i++) {
		if(isPrime(arr[i])) {
			printf("%d" , arr[i]);
		}
		printf("\n");
    }
}