#include<stdio.h>
int main()
{
	int arr[100], size, i, sum = 0;
	printf("Enter the size of the array: ");
	scanf("%d" , &size);
	printf("Enter the elements of the array: ");
	for(int i = 0; i < size; i++) {
		scanf("%d" , arr[i]);
		sum = sum + arr[i];
	}
	printf("Sum of all elements is: %d\n" , sum);
}