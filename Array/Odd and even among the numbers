#include<stdio.h>
int main()
{
	int arr[100], size, i;
	printf("Enter the size of the array: ");
	scanf("%d" , &size);
	printf("Enter the elements of the array: ");
	for(int i = 0; i < size; i ++) {
		scanf("%d" , &arr[i]);
	}
	printf("Even and Odd numbers: \n");
	for(int i = 0; i < size; i++) {
		if(arr[i] % 2 == 0) {
			printf("%d is Even\n" , arr[i]);
		} else {
			printf("%d is Odd\n" , arr[i]);
		}
	}
}