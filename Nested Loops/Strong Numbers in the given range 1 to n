#include<stdio.h>
int main()
{
	int num, i, temp, sum, digit, fact;
	
	printf("Enter the value of num: ");
	scanf("%d" , &num);
	
	printf("Strong numbers from 1 to %d are:\n" , num);
	
	for(int i = 1; i <= num; i++){
		temp = i;
		sum = 0;
		while(temp > 0) {
			digit = temp % 10;
			fact = 1;
			int j;
			for(j = 1; j <= digit; j++) {
				fact = fact * j;
			}
			sum = sum + fact;
			temp = temp / 10;
		}
		if(sum == i) {
			printf("%d\n" , i);
		}
	}
}