#include<stdio.h>
int main()
{
	int num, i, temp, sum, digit;
	printf("Enter a positive integer no: ");
	scanf("%d" , &num);
	
	printf("Armstrong numbers from 1 to %d are: \n" , num);
	
	for(int i = 1; i < num; i++){
		sum = 0;
	    temp = i;
	}
	
	while(temp > 0) {
		digit = temp % 10;
		sum = sum + (digit * digit * digit);
		temp = temp / 10;
	}
	
	if(i == sum) {
		printf("%d\n" , num);
	}
}