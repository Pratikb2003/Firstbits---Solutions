#include<stdio.h>
int main()
{
	int num, i, j, is_prime;
		
	printf("Enter the upper limit to find prime numbers: ");
	scanf("%d" , &num);
	
	for(int i = 2; i <= num; i++) {
	is_prime = 1;
	}
	
	for(int j = 2; j < i; j++) {
	   if(i % j == 0) {
		   is_prime = 0;
		break;
	   }
	}

	if(is_prime == 1) {
		printf("%d " , i);
	}
}